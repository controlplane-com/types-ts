{
  "name": "@controlplane/schema",
  "version": "1.0.6",
  "description": "Control Plane Corporation Schema",
  "scripts": {
    "build": "tsc",
    "build:turbo": "turbo run build --no-daemon",
    "clean": "rm -fr build/",
    "prettier:check": "prettier --parser typescript --list-different 'src/**/*.{ts,}'",
    "prettier:write": "prettier --parser typescript --write 'src/**/*.{ts,}'"
  },
  "keywords": [
    "Control Plane",
    "Control Plane Corporation",
    "cpln"
  ],
  "dependencies": {},
  "devDependencies": {
    "typescript": "4.9.4"
  },
  "files": [
    "build/src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "build/src/*"
      ]
    }
  },
  "exports": {
    "./*": "./build/src/interfaces/*.js"
  },
  "engines": {
    "node": ">= 16"
  }
}
