{
  "name": "@controlplane/schema",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:turbo": "turbo run build --no-daemon",
    "start": "pnpm build:turbo && node --inspect=19025 build/src/main.js",
    "test": "        jest --verbose --testPathIgnorePatterns test/",
    "clean": "rm -fr build/",

    "prettier:check": "prettier --parser typescript --list-different 'src/**/*.{ts,}'",
    "prettier:write": "prettier --parser typescript --write 'src/**/*.{ts,}'"
  },
  "dependencies": {
    
  },
  "devDependencies": {
    "typescript": "4.9.4"
  },
  "files": [
    "build/src"
  ],
  "engines": {
    "node": ">= 16"
  }
}